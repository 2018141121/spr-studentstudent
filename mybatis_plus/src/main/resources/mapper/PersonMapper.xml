<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codel.mybatis_plus.mapper.PersonMapper">

    <select id="getPersonAll" resultType="com.codel.mybatis_plus.pojo.Person">
        select *
        from person;
    </select>

    <select id="selectPageVo" resultType="com.codel.mybatis_plus.pojo.Person">
        SELECT id, name
        FROM person
        WHERE state = #{state}
    </select>

</mapper>