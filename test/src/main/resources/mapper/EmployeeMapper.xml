<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codel.test.mapper.EmployeeMapper">


    <select id="getEmployee" resultType="java.lang.Integer">
        select count(*)
        from vhr.employee
        where wedlock = #{wedlock};
    </select>
</mapper>
